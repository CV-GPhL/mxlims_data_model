        "LogisticalSample": {
            "description": "Base class for MXLIMS Logistical Samples\n\ndescribing Sample containers and locations\n(from Dewars and Plates to drops, pins and crystals)",
            "properties": {
                "uuid": {
                    "description": "Permanent unique identifier string",
                    "title": "Uuid",
                    "type": "string"
                },
                "extensions": {
                    "description": "Keyword-value dictionary string:Any of extensions.Use is accepted but discouraged",
                    "title": "Extensions",
                    "type": "object"
                },
                "namespace_extensions": {
                    "additionalProperties": {
                        "$ref": "#/$defs/BaseModel"
                    },
                    "description": "Keyword-value dictionary of namespaced extension. Key is an organisation identifier (e.g. 'GPhL' or 'ESRF'), value is a Pydantic model defined by this organisation.",
                    "title": "Namespace Extensions",
                    "type": "object"
                },
                "sample_id": {
                    "anyOf": [
                        {
                            "type": "string"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "String UUID for the sample preparation that applies to this LogisticalSample and all its contents",
                    "title": "Sample Id"
                },
                "container": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/LogisticalSample"
                        },
                        {
                            "type": "null"
                        }
                    ],
                    "default": null,
                    "description": "The LogisticalSample containing this one"
                },
                "contents": {
                    "description": "LogisticalSamples contained in this one",
                    "items": {
                        "$ref": "#/$defs/LogisticalSample"
                    },
                    "title": "Contents",
                    "type": "array"
                }
            },
            "title": "LogisticalSample",
            "type": "object"
